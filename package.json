{
  "name": "GraphQL-Explorer-Server",
  "version": "1.0.0",
  "description": "API Usage of GraphQL Stack",
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon ./server/app.ts",
    "prebuild": "npm run genql && node ./scripts/clean.js --server && npm run prisma",
    "build": "cross-env NODE_ENV=production tsc",
    "postbuild": "node ./scripts/prisma/copy.js && npm run prisma:db:prod && node ./dist/prisma/seed.js",
    "build:vercel": "npm run genql && cross-env NODE_ENV=production node ./scripts/clean.js --vercel && tsc --build vercel-tsconfig.json",
    "clean": "node ./scripts/clean.js --server --vercel",
    "test": "cross-env NODE_ENV=test jest --forceExit",
    "prestart": "npm run build",
    "start": "cross-env NODE_ENV=production nodemon ./dist/app.js",
    "pm2": "npm run build && pm2 start ecosystem.config.js",
    "stress-test": "node ./scripts/st-main.js --time 1000 * 60",
    "voyager": "serve -s voyager/local.html",
    "gen:code": "graphql-codegen --config codegen.yml",
    "gen:docs": "rimraf -rf ./docs && graphdoc --force",
    "gen": " node ./scripts/gen/main.js --docs --code",
    "serve:docs": "npm run gen:docs && serve -s docs",
    "commit": "git add -A && git-cz",
    "typecheck": "tsc --noEmit",
    "prod:vercel": "vc -prod",
    "prettier": "prettier --write ./**/*.ts ./**/*.js ./**/*.json ./**/*.graphql",
    "seed:config": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "cross-env NODE_ENV=development ts-node ./node_modules/typeorm-seeding/dist/cli.js seed",
    "genql": "genql --schema ./server/typegraphql/schema.graphql --output ./server/genql-generated",
    "prisma": "npm run prisma:db && npm run prisma:generate",
    "prisma:db": "prisma db push --preview-feature",
    "prisma:db:prod": "prisma db push --schema=./dist/prisma/schema.prisma --preview-feature",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --preview-feature",
    "prisma:migrate:reset": "prisma migrate reset --preview-feature",
    "prisma:seed": "ts-node ./server/prisma/seed.ts",
    "generate": "ts-node ./scripts/command/index.ts generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linbudu599/GraphQL-Explorer-Server.git"
  },
  "keywords": [
    "GraphQL",
    "Parcel",
    "TypeGraphQL",
    "Apollo-Server",
    "Apollo-Client"
  ],
  "graphdoc": {
    "endpoint": "http://localhost:4000/graphql",
    "output": "./docs"
  },
  "prisma": {
    "schema": "server/prisma/schema.prisma"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "npm run build && npm run test"
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "author": "linbudu599",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/linbudu599/GraphQL-Explorer-Server/issues"
  },
  "homepage": "https://github.com/linbudu599/GraphQL-Explorer-Server#readme",
  "dependencies": {
    "@2fd/graphdoc": "^2.4.0",
    "@genql/runtime": "^2.3.3",
    "@prisma/cli": "^2.15.0",
    "@prisma/client": "^2.15.0",
    "@saeris/apollo-server-vercel": "^0.4.0",
    "apollo-datasource-rest": "^0.9.5",
    "apollo-server": "^2.17.0",
    "apollo-server-koa": "^2.17.0",
    "apollo-server-plugin-base": "^0.10.2",
    "apollo-server-plugin-response-cache": "^0.5.7",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dataloader": "^2.0.0",
    "esbuild": "^0.8.31",
    "graphql": "^15.3.0",
    "graphql-modules": "^1.2.0",
    "graphql-scalars": "^1.5.0",
    "graphql-tag": "^2.11.0",
    "graphql-voyager": "^1.0.0-rc.31",
    "graphqldoc": "^2.5.2",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.13.0",
    "sqlite3": "^5.0.1",
    "type-graphql": "^1.0.0",
    "type-graphql-dataloader": "^0.3.7",
    "typedi": "^0.8.0",
    "typeorm": "^0.2.25",
    "typeorm-seeding": "^1.6.1",
    "typeorm-typedi-extensions": "^0.2.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@genql/cli": "^2.3.3",
    "@graphql-codegen/cli": "^1.19.2",
    "@graphql-codegen/fragment-matcher": "^2.0.0",
    "@graphql-codegen/introspection": "^1.18.0",
    "@graphql-codegen/time": "^2.0.1",
    "@graphql-codegen/typescript": "^1.17.11",
    "@graphql-codegen/typescript-operations": "^1.17.10",
    "@graphql-codegen/typescript-react-apollo": "^2.1.1",
    "@graphql-codegen/typescript-resolvers": "^1.17.11",
    "@graphql-codegen/typescript-type-graphql": "^1.18.1",
    "@ice/spec": "^1.0.1",
    "@types/bcrypt": "^3.0.0",
    "@types/faker": "^5.1.5",
    "@types/inquirer": "^7.3.1",
    "@types/jest": "^26.0.13",
    "@types/mkdirp": "^1.0.1",
    "@types/node": "^14.6.3",
    "apollo-server-testing": "^2.17.0",
    "babel-jest": "^26.3.0",
    "chalk": "^4.1.0",
    "commitizen": "^4.2.3",
    "commitlint-config-cz": "^0.13.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.3.0",
    "dotenv": "^8.2.0",
    "esbuild-node-tsc": "^1.0.5",
    "fs-extra": "^0.30.0",
    "husky": "^4.3.0",
    "inquirer": "^7.3.3",
    "jest": "^26.4.2",
    "minimist": "^1.2.5",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "pm2": "^4.5.1",
    "prettier": "^2.2.0",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "serve": "^11.3.2",
    "superagent": "^6.1.0",
    "ts-jest": "^26.3.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.3"
  }
}
